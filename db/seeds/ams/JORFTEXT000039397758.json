          "id": "e13af9bAb942",
          "outer_alineas": [
            {
              "text": "Tous les sous-produits et déchets produits par l'installation sont entreposés dans des contenants étanches et dans des conditions prévenant toute dégradation qui remettrait en cause leur valorisation ou élimination appropriée."
            },
            {
              "text": "Les effluents collectés et confinés sur place sont transportés hors site chaque jour dans des installations dûment appropriées pour leur traitement."
            }
          ],
          "parametrization": {
            "potential_inapplicabilities": [],
            "potential_modifications": [],
            "warnings": []
          },
          "reference_str": "Art. 19",
          "sections": [],
          "title": {
            "text": "Article 19 - Gestion des sous-produits et des déchets"
          }
        },
        {
          "annotations": {
            "topic": null
          },
          "applicability": {
            "active": true,
            "modified": false,
            "previous_version": null,
            "warnings": []
          },
          "id": "C4c0b6dd513E",
          "outer_alineas": [
            {
              "text": "Le brûlage des déchets liquides, solides et gazeux est interdit sur le site."
            }
          ],
          "parametrization": {
            "potential_inapplicabilities": [],
            "potential_modifications": [],
            "warnings": []
          },
          "reference_str": "Art. 20",
          "sections": [],
