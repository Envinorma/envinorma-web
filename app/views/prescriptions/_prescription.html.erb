<div class='<%= "prescription_#{prescription.id}" %>' >
    <h6>
      <%= prescription.text_reference %>
    </h6>
    <p>
      <strong>
        <%= prescription.reference %>
      </strong>
    <br>
      <% prescription.content.split("\n").each do |line| %>
        <%= line %>
        <br>
      <% end %>
    </p>
    <p>

      <%= link_to "Supprimer", prescription, remote: true, method: :delete %>
    </p>
</div>
